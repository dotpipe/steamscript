{
    "type": "menu",
    "title": "TODO & Feedback Dashboard",
    "dataSource": ["todo.json"],
    "showPrompt": false,
    "ownershipProtocol": true,
    "useArrows": true,
    "options": [
        { "label": "[Add New Task]", "next": { "type": "textbox", "prompt": "Enter new task:", "onSubmit": "addTask" } },
        { "label": "[View All Tasks]", "next": {
            "type": "menu",
            "title": "All Tasks",
            "options": "[{{todos}}]",
            "useArrows": true,
            "onSelect": "viewTask"
        }}
    ],
    "footer": "Up/Down: Move  Enter: Select  F12: Exit"
}